package t01;

public class Test5 {
	public static void main(String[] args) {
		int su = 30;
		System.out.printf("숫//자 : %d\n", su);
		
		int su2 = 50;  // 정수선언 변수 = //선언문 
		System.out.println("숫자2 : " + su2);  //실행문
		
		int su3, su4;
		su3 = 10;
		su4 = 20;
		System.out.println("su3 : " + su3 + " , su4 : " + su4);
		
		int su5 = 5, su6 = 6;
		System.out.println("su5 : " + su5 + " , su6 : " + su6);
		
		int _su7 = 100, $su8 = 200, su$9 = 300;
		System.out.println("_su7 : " + _su7 + " , $su8 : " + $su8);
	}
}
